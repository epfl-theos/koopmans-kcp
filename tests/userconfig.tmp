[user]
benchmark = git
date_fmt = %d%m%y
diff = vimdiff

[KCP]
exe = XXXXXX/tests/run-kcp.sh
extract_program = XXXXXX/tests/extract-kcp.sh
inputs_args = ('*.in', '')
run_cmd_template = tc.program tc.input tc.output tc.error
tolerance = ( (8.0e-4, 1.0e-4, 'e1'),
              (8.0e+0, 1.0e+1, 'n1'),
              (1.0e-3, None,   'f1'),
              (2.0e+0, None,   'p1'),
              (8.0e-2, 2.0e-2, 'ef1'),
              (1.0e-2, 1.0e-2, 'eh1'),
              (1.0e-2, 2.0e-2, 'ehl1'),
              (2.0e-1, None,   'band'),
              (1.0e-2, 1.0e-2, 'tf1'),
              (1.0e-1, None,   'vol'),
              (5.0e-3, None,   'geom') )
